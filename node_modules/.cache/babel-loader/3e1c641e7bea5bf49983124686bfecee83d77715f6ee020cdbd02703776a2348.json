transform: `translate(0,${dy / dampen}px)`\r\n  }\r\n})\r\n\r\nfunction startDrag(e) {\r\n  e = e.changedTouches ? e.changedTouches[0] : e\r\n  isDragging = true\r\n  start.x = e.pageX\r\n  start.y = e.pageY\r\n}\r\n\r\nfunction onDrag(e) {\r\n  e = e.changedTouches ? e.changedTouches[0] : e\r\n  if (isDragging) {\r\n    c.x = headerHeight + (e.pageX - start.x)\r\n    const dy = e.pageY - start.y\r\n    const dampen = dy > 0 ? 1.5 : 4\r\n    c.y = headerHeight + dy / dampen\r\n  }\r\n}\r\n\r\nfunction stopDrag() {\r\n  if (isDragging) {\r\n    isDragging = false\r\n    dynamics.animate(\r\n        c,\r\n        { x: headerHeight, y: headerHeight },\r\n        { type: dynamics.spring, duration: 700, friction: 280 }\r\n    )\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n.logo {\r\n  animation: bounce 1s infinite;\r\n}\r\n\r\n@keyframes bounce {\r\n  0%, 100% {\r\n    transform: translateY(0);\r\n  }\r\n  50% {\r\n    transform: translateY(-10px);\r\n  }\r\n}\r\n</style>"],"mappings":";OAEwBA,UAAwB;;EADzCC,SAAS,EAAC;AAAM;gCACnBC,mBAAA,CAA8D;EAAzDC,GAAG,EAAC,UAAU;EAACC,GAAwB,EAAxBJ,UAAwB;EAACC,SAAS,EAAC;;;;;uBADzDI,mBAAA,CAGM,OAHNC,UAGM,GAFJC,UAA8D,EAC9DC,YAAA,CAAuCC,qBAAA;IAA3BC,GAAG,EAAC;EAAqB,G"},"metadata":{},"sourceType":"module","externalDependencies":[]}