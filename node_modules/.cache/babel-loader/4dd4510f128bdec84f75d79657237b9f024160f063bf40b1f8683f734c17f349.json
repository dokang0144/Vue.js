n\r\n\r\nfunction stopDrag() {\r\n  if (isDragging) {\r\n    isDragging = false\r\n    if (c.y > headerHeight + 100) {\r\n      setTimeout(() => {\r\n        router.push('/login')\r\n      }, 1000);\r\n    } else {\r\n      dynamics.animate(\r\n          c,\r\n          { x: headerHeight, y: headerHeight },\r\n          { type: dynamics.spring, duration: 700, friction: 280 }\r\n      )\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<template>\r\n  <div\r\n      class=\"draggable\"\r\n      :class=\"transitionClass\"\r\n      @mousedown=\"startDrag\"\r\n      @touchstart=\"startDrag\"\r\n      @mousemove=\"onDrag\"\r\n      @touchmove=\"onDrag\"\r\n      @mouseup=\"stopDrag\"\r\n      @touchend=\"stopDrag\"\r\n      @mouseleave=\"stopDrag\"\r\n  >\r\n    <svg class=\"bg\" width=\"320\" height=\"560\">\r\n      <path :d=\"headerPath\" fill=\"yellowgreen\"></path>\r\n    </svg>\r\n    <div class=\"header\">Drag Me</div>\r\n    <div class=\"content\" :style=\"contentPosition\">{{ contentText }}</div>\r\n  </div>\r\n</template>\r\n\r\n\r\n<style scoped>\r\n.draggable {\r\n  background-color: #fff;\r\n  box-shadow: 0 4px 16px rgba(0, 0, 0, 0.15);\r\n  width: 320px;\r\n  height: 250px;\r\n  overflow: hidden;\r\n  margin: 30px auto;\r\n  position: relative;\r\n  text-align: center;\r\n  font-size: 14px;\r\n  font-weight: 300;\r\n  user-select: none;\r\n  border-radius: 8px;\r\n}\r\n.bg {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  z-index: 0;\r\n}\r\n.header,\r\n.content {\r\n  position: relative;\r\n  z-index: 1;\r\n  padding: 30px;\r\n  box-sizing: border-box;\r\n}\r\n.header {\r\n  color: #fff;\r\n  height: 120px;\r\n  font-size: 2em;\r\n  font-weight: bold;\r\n}\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n.page-slide {\r\n  /* 페이지가 왼쪽으로 슬라이드 되는 모션 */\r\n  position: absolute;\r\n  top: 0;\r\n  left: 100%;\r\n  width: 100%;\r\n  height: 100%;\r\n  transition: transform 0.5s;\r\n}\r\n.page-slide-enter-active,\r\n.page-slide-leave-active {\r\n  transition: transform 0.5s;\r\n}\r\n.page-slide-enter,\r\n.page-slide-leave-to {\r\n  transform: translateX(-100%);\r\n}\r\n</style>"],"mappings":";;;EA2FSA,KAAK,EAAC,IAAI;EAACC,KAAK,EAAC,KAAK;EAACC,MAAM,EAAC;;;gEAGnCC,mBAAA,CAAiC;EAA5BH,KAAK,EAAC;AAAQ,GAAC,SAAO;;;uBAd7BI,mBAAA,CAgBM;IAfFJ,KAAK,EAAAK,eAAA,EAAC,WAAW,EACTC,IAAA,CAAAC,eAAe;IACtBC,WAAS,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEJ,IAAA,CAAAK,SAAA,IAAAL,IAAA,CAAAK,SAAA,IAAAD,IAAA,CAAS;IACpBE,YAAU,EAAAH,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEJ,IAAA,CAAAK,SAAA,IAAAL,IAAA,CAAAK,SAAA,IAAAD,IAAA,CAAS;IACrBG,WAAS,EAAAJ,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEJ,IAAA,CAAAQ,MAAA,IAAAR,IAAA,CAAAQ,MAAA,IAAAJ,IAAA,CAAM;IACjBK,WAAS,EAAAN,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEJ,IAAA,CAAAQ,MAAA,IAAAR,IAAA,CAAAQ,MAAA,IAAAJ,IAAA,CAAM;IACjBM,SAAO,EAAAP,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEJ,IAAA,CAAAW,QAAA,IAAAX,IAAA,CAAAW,QAAA,IAAAP,IAAA,CAAQ;IACjBQ,UAAQ,EAAAT,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEJ,IAAA,CAAAW,QAAA,IAAAX,IAAA,CAAAW,QAAA,IAAAP,IAAA,CAAQ;IAClBS,YAAU,EAAAV,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEJ,IAAA,CAAAW,QAAA,IAAAX,IAAA,CAAAW,QAAA,IAAAP,IAAA,CAAQ;qBAEvBN,mBAAA,CAEM,OAFNgB,UAEM,GADJjB,mBAAA,CAAgD;IAAzCkB,CAAC,EAAEf,IAAA,CAAAgB,UAAU;IAAEC,IAAI,EAAC;0CAE7BC,UAAiC,EACjCrB,mBAAA,CAAqE;IAAhEH,KAAK,EAAC,SAAS;IAAEyB,KAAK,EAAAC,eAAA,CAAEpB,IAAA,CAAAqB,eAAe;sBAAKrB,IAAA,CAAAsB,WAAW,wB"},"metadata":{},"sourceType":"module","externalDependencies":[]}