{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { mapActions, mapGetters } from 'vuex';\nexport default {\n  data() {\n    return {\n      show: false,\n      user_id: '',\n      user_pw: ''\n    };\n  },\n  mounted() {\n    this.show = true;\n  },\n  methods: {\n    ...mapActions(['login']),\n    async fnLogin() {\n      if (this.user_id === '') {\n        alert('ID를 입력하세요.');\n        return;\n      }\n      if (this.user_pw === '') {\n        alert('비밀번호를 입력하세요.');\n        return;\n      }\n      try {\n        let loginResult = await this.login({\n          user_id: this.user_id,\n          user_pw: this.user_pw\n        });\n        if (loginResult) this.goToPages();\n      } catch (err) {\n        if (err.message.indexOf('Network Error') > -1) {\n          alert('서버에 접속할 수 없습니다. 상태를 확인해주세요.');\n        } else {\n          alert('로그인 정보를 확인할 수 없습니다.');\n        }\n      }\n    },\n    goToPages() {\n      this.$router.push({\n        name: 'List'\n      });\n    }\n  },\n  computed: {\n    ...mapGetters({\n      errorState: 'getErrorState'\n    })\n  }\n};","map":{"version":3,"names":["mapActions","mapGetters","data","show","user_id","user_pw","mounted","methods","fnLogin","alert","loginResult","login","goToPages","err","message","indexOf","$router","push","name","computed","errorState"],"sources":["C:\\Users\\LENOVO\\Documents\\GitHub\\Vue.js\\src\\views\\common\\PageLogin.vue"],"sourcesContent":["<template>\r\n  <div style=\"display: flex; align-items: center; justify-content: center; height: 100vh;\">\r\n    <Transition name=\"bounce\">\r\n    <div v-if=\"show\" style=\"margin-top: 20px; text-align: center;\">\r\n      <h2>돌아오신 것을 환영해요!</h2>\r\n      <p style=\"color: #a0a0a0; font-size: 18px; margin: 0\">다시 만나다니 너무 반가워요!</p>\r\n      <div id=\"loginForm\">\r\n        <form @submit.prevent=\"fnLogin\">\r\n          <p>\r\n            <input class=\"w3-input\" name=\"uid\" placeholder=\"아이디를 입력해주세요\" v-model=\"user_id\"><br>\r\n          </p>\r\n          <p>\r\n            <input name=\"password\" class=\"w3-input\" placeholder=\"비밀번호를 입력해주세요\" v-model=\"user_pw\" type=\"password\" style=\"-webkit-text-security: square\">\r\n          </p>\r\n          <p>\r\n            <button type=\"submit\" class=\"w3-button w3-green w3-round\">Login</button>\r\n          </p>\r\n          <p>계정이 필요한가요? <b><router-link to=\"/signup\" style=\"color: #42b983\">가입하기</router-link></b></p>\r\n        </form>\r\n      </div>\r\n    </div>\r\n    </Transition>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {mapActions, mapGetters} from 'vuex'\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      show: false,\r\n      user_id: '',\r\n      user_pw: ''\r\n    }\r\n  },\r\n  mounted() {\r\n    this.show = true;\r\n  },\r\n  methods: {\r\n    ...mapActions(['login']),\r\n\r\n    async fnLogin() {\r\n      if (this.user_id === '') {\r\n        alert('ID를 입력하세요.')\r\n        return\r\n      }\r\n\r\n      if (this.user_pw === '') {\r\n        alert('비밀번호를 입력하세요.')\r\n        return\r\n      }\r\n\r\n      try {\r\n        let loginResult = await this.login({user_id: this.user_id, user_pw: this.user_pw})\r\n        if (loginResult) this.goToPages()\r\n      } catch (err) {\r\n        if (err.message.indexOf('Network Error') > -1) {\r\n          alert('서버에 접속할 수 없습니다. 상태를 확인해주세요.')\r\n        } else {\r\n          alert('로그인 정보를 확인할 수 없습니다.')\r\n        }\r\n      }\r\n    },\r\n    goToPages() {\r\n      this.$router.push({\r\n        name: 'List'\r\n      })\r\n    }\r\n  },\r\n  computed: {\r\n    ...mapGetters({\r\n      errorState: 'getErrorState'\r\n    })\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n#loginForm {\r\n  width: 450px;\r\n  margin: auto;\r\n}\r\n\r\ninput[type=password] {font-family:'Malgun gothic', dotum, sans-serif;}\r\n\r\n::placeholder { /* Chrome, Firefox, Opera, Safari 10.1+ */\r\n  font-family: 'NanumSquare', sans-serif;\r\n}\r\n\r\n.bounce-enter-active {\r\n  animation: bounce-in 0.5s;\r\n}\r\n.bounce-leave-active {\r\n  animation: bounce-in 0.5s reverse;\r\n}\r\n@keyframes bounce-in {\r\n  0% {\r\n    transform: scale(0);\r\n  }\r\n  50% {\r\n    transform: scale(1.25);\r\n  }\r\n  100% {\r\n    transform: scale(1);\r\n  }\r\n}\r\n</style>"],"mappings":";AA0BA,SAAQA,UAAU,EAAEC,UAAU,QAAO,MAAK;AAE1C,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,KAAK;MACXC,OAAO,EAAE,EAAE;MACXC,OAAO,EAAE;IACX;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACH,IAAG,GAAI,IAAI;EAClB,CAAC;EACDI,OAAO,EAAE;IACP,GAAGP,UAAU,CAAC,CAAC,OAAO,CAAC,CAAC;IAExB,MAAMQ,OAAOA,CAAA,EAAG;MACd,IAAI,IAAI,CAACJ,OAAM,KAAM,EAAE,EAAE;QACvBK,KAAK,CAAC,YAAY;QAClB;MACF;MAEA,IAAI,IAAI,CAACJ,OAAM,KAAM,EAAE,EAAE;QACvBI,KAAK,CAAC,cAAc;QACpB;MACF;MAEA,IAAI;QACF,IAAIC,WAAU,GAAI,MAAM,IAAI,CAACC,KAAK,CAAC;UAACP,OAAO,EAAE,IAAI,CAACA,OAAO;UAAEC,OAAO,EAAE,IAAI,CAACA;QAAO,CAAC;QACjF,IAAIK,WAAW,EAAE,IAAI,CAACE,SAAS,EAAC;MAClC,EAAE,OAAOC,GAAG,EAAE;QACZ,IAAIA,GAAG,CAACC,OAAO,CAACC,OAAO,CAAC,eAAe,IAAI,CAAC,CAAC,EAAE;UAC7CN,KAAK,CAAC,6BAA6B;QACrC,OAAO;UACLA,KAAK,CAAC,qBAAqB;QAC7B;MACF;IACF,CAAC;IACDG,SAASA,CAAA,EAAG;MACV,IAAI,CAACI,OAAO,CAACC,IAAI,CAAC;QAChBC,IAAI,EAAE;MACR,CAAC;IACH;EACF,CAAC;EACDC,QAAQ,EAAE;IACR,GAAGlB,UAAU,CAAC;MACZmB,UAAU,EAAE;IACd,CAAC;EACH;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}